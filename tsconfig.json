{
    "compilerOptions": {
      "forceConsistentCasingInFileNames": true,
      "sourceMap": true,
      "outDir": "dist",
      "strict": true,
      "lib": ["esnext"],
      "esModuleInterop": true,
      "target": "ES2020",
      "module": "CommonJS",
      "rootDir": "./",
      "resolveJsonModule": true,
      "skipLibCheck": true,
    },
    "include": ["**/*.ts"],
    "exclude": ["node_modules"]
  }
  